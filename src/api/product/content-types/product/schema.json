{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "kcal": {
      "type": "string",
      "required": true
    },
    "prots": {
      "type": "string"
    },
    "fats": {
      "type": "string"
    },
    "carbs": {
      "type": "string"
    },
    "menu": {
      "type": "component",
      "repeatable": true,
      "component": "menu.menu"
    }
  }
}
